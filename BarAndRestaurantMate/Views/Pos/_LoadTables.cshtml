@model BarAndRestaurantMate.Models.IndexViewModel

@foreach (var t in Model.MyTables)
{
    if (t.Id == Model.TableId)
    {
        <span class="Hold selectedHold" id="@t.Id" onclick="SelectHold(@t.Id)">@t.TableName<span id="Time"></span></span>
    }
    else
    {
        <span class="Hold" id="@t.Id" onclick="SelectHold(@t.Id)">@t.TableName<span id="Time"></span></span>
    }
    
}


