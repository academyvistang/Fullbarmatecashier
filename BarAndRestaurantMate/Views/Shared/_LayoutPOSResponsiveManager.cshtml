<body>
    <!-- Navigation -->
    <div class="wrap" style="margin-left:5%;">

        <a href="@Url.Action("IndexNew", "Pos")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Point Of Sale" style="width:100%; font-size:10px; text-align:center;" title="Point Of Sale" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/Pos.png" />
                    <div class="titleBox">Point Of Sale</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("AllOrders", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Table Orders" style="width:100%; font-size:10px; text-align:center;" title="Table Orders" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/tableNew.png" />
                    <div class="titleBox">Table Orders</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("AllOrdersEmpty", "Guest")">
