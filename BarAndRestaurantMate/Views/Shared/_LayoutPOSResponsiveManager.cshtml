                    <img src="~/images/icons/edit_tasks-64.png" />
                    <div class="titleBox">Corrections</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("ApproveGalleryIndex", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Approve Gallery" style="width:100%; font-size:10px; text-align:center;" title="Approve Gallery" />
