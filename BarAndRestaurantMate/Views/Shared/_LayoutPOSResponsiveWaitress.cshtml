<body>
    <!-- Navigation -->
    <div class="wrap" style="margin-left:5%;">


        <a href="@Url.Action("WaitressTables", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Tables" style="width:100%; font-size:10px; text-align:center;" title="Tables" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/table1.jpeg" />
                    <div class="titleBox">Tables</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("WaitressOrders", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Table Orders" style="width:100%; font-size:10px; text-align:center;" title="Orders awaiting processing" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/order.png" />
                    <div class="titleBox">Table Orders</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("UnusedTables", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Delete Table" style="width:100%; font-size:10px; text-align:center;" title="Delete Table" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/deletetable.png" />
                    <div class="titleBox">Delete Table</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("WaiterCollections", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Collections" style="width:100%; font-size:10px; text-align:center;" title="Collections" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/Collections.png" />
                    <div class="titleBox">Collections</div>
                </div>
            </div>
        </a>


        <a href="@Url.Action("RequestBill", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Request Bill" style="width:100%; font-size:10px; text-align:center;" title="Request Bill" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/Phone.png" />
                    <div class="titleBox">Request Bill!</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("SwapTables", "Guest")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Swap Tables" style="width:100%; font-size:10px; text-align:center;" title="Swap Tables" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/flip_horizontal-64.png" />
                    <div class="titleBox">Swap Tables</div>
                </div>
            </div>
        </a>

        <a href="@Url.Action("LogOff", "Account")">
            <div class="box">
                <div style="width:95%;padding-left:5%;">
                    <input type="text" value="Log Off" style="width:100%; font-size:10px; text-align:center;" title="Log Off" />
                </div>
                <div class="boxInner">
                    <img src="~/images/icons/preferences-desktop-cryptography.png" />
                    <div class="titleBox">Log Off</div>
                </div>
            </div>
        </a>



    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 left-side">
                @RenderBody()
            </div>
        </div>
    </div>

    <!-- /.container -->
    <!-- /.Modal -->
