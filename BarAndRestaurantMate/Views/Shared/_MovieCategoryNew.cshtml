@model BarAndRestaurantMate.Models.GuestRoomAccountViewModel

<style>

    /*------------------Image Carousel-----------------------*/

    .imageContainer {
        /* height: 200px; */
        width: 100%;
        margin: 0 0 0 0px;
        background: #404040;
        z-index: 0;
        overflow: auto;
        overflow-y: hidden;
    }

    ul.thumb {
        float: left;
        list-style: none;
        margin: 0;
        padding: 10px;
        width: 1700px;
    }

        ul.thumb li {
            margin: 0;
            padding: 5px;
            float: left;
            position: relative;
            width: 160px;
            height: 110px;
        }

            ul.thumb li img {
                width: 150px;
                height: 150px;
                border: 1px solid #303030;
                padding: 0px;
                background: White;
                position: absolute;
                left: 0;
                top: 0;
                -ms-interpolation-mode: bicubic;
            }

                ul.thumb li img.hover {
                    /*background: url('Images/thumb_bg.png') no-repeat center center; */
                    border: none;
                }


    .CarouselImage {
        cursor: pointer;
        cursor: hand;
        width: 150px;
        height: 150px;
        border: 1px solid #303030;
        padding: 0px;
        background: White;
        position: absolute;
        left: 0;
        top: 0;
        -ms-interpolation-mode: bicubic;
    }

    /*------------------End Of Image Carousel-----------------------*/



    /* ==============================================
					Theme White 
   ============================================== */
    #gallery-black {
        height: 215px;
        width: 100%;
        margin: 0 0 0 0px;
        background: #303030;
        z-index: 0;
        overflow: auto;
        overflow-y: hidden;
    }

        #gallery-black .imageContainer {
            float: left;
            list-style: none;
            margin: 0;
            padding: 10px;
            width: 100%;
        }



        #gallery-black .image-block {
            width: 155px;
            margin: 0px 8px;
            float: left;
            height: 250px;
        }

        #gallery-black .reflection {
            position: relative;
        }

            #gallery-black .reflection img {
                -webkit-transform: scaleY(-1);
                -moz-transform: scaleY(-1);
                -ms-transform: scaleY(-1.05);
                -o-transform: scaleY(-1);
                transform: scaleY(-1);
                filter: flipv;
                opacity: 0.20;
                filter: alpha(opacity='20');
            }

        #gallery-black .overlay {
            position: absolute;
            top: 0px;
            left: 0px;
            width: 155px;
            height: 250px;
            background-image: -moz-linear-gradient(center bottom, rgb(255,255,255) 60%, rgba(255,255,255,0) 75%);
            background-image: -o-linear-gradient(rgba(255,255,255,0) 25%, rgb(255,255,255) 40%);
            background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.60, rgb(255,255,255)), color-stop(0.75, rgba(255,255,255,0)));
            filter: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColor=0, EndColorStr=#ffffff);
        }


    #roundedDivReport {
        padding: 0px 0px 15px 15px;
        margin: 10px 0px 0px 0px;
        background-color: #f7f7f7;
        border-radius: 15px;
        width: 460px;
    }


        #roundedDivReport h1 {
            font: 25px 'PT Sans', Arial, Helvetica, sans-serif;
            margin-bottom: 0px;
            color: Gray;
        }

        #roundedDivReport h2 {
            font: 25px 'PT Sans', Arial, Helvetica, sans-serif;
            margin-bottom: 0px;
            text-align: center;
            color: Blue;
        }

    .divDescription {
        padding: 0px 0px 0px 42px;
    }


        .divDescription p {
            font-family: 'PT Sans', Arial;
            font-size: 13px;
            color: #333333;
        }


        .divDescription a {
            margin: 75px 75px 75px 0px;
            color: #336699;
        }

        .divDescription img {
            margin: 2px 5px 0px 0px;
            border-style: none;
        }


    #roundedDivPresentation {
        padding: 15px 0px 15px 15px;
        margin: 10px 0px 0px 0px;
        background-color: #f7f7f7;
        border-radius: 15px;
        width: 365px;
        float: left;
    }


    .PresentationPictureDiv {
        margin-top: 20px;
        width: 305px;
    }

        .PresentationPictureDiv img {
            height: 105px;
            width: 125px;
        }

        .PresentationPictureDiv p {
            padding: 0px 0px 0px 0px;
            margin: 0px 0px 0px 0px;
        }


    #roundedDivPresentation img {
        margin: 0px 0px 0px 0px;
    }

    .MegaMenuHeaderSummaries {
        text-align: left;
        font-family: 'PT Sans', Arial;
        color: Gray;
        font-size: 14px;
        font-weight: bold;
        line-height: 18px;
    }


    #MonthlyPresentationLayout {
        float: left;
        height: 400px;
        width: 735px;
    }



    #ListViewPresentationContainer {
        float: left;
        height: 308px;
        width: 763px;
        z-index: 0;
        overflow: auto;
        overflow-y: hidden; /*border-top: 2px solid #f7f7f7;     border-right: 2px solid #f7f7f7;         border-left: 2px solid #f7f7f7;     background-color:#FFFFFF;*/
    }

    /*#ListViewPresentationContainer .presContainer
{
    float: left;
    list-style: none;
    margin: 0;
    padding:5px 5px 0px 2px;
    width: 900px;
    margin-bottom:15px;   
}*/

    .presContainer {
        float: left;
        list-style: none;
        margin: 0;
        padding: 5px 5px 0px 2px; /*width: 900px;*/
        margin-bottom: 15px;
        height: 300px;
    }

    .PresentationBottomContainer {
        clear: both;
        margin: 0px 0px 0px 0px;
        background-color: #E8E8E8;
        -moz-border-radius-bottomright: 15px;
        border-bottom-right-radius: 15px;
        -moz-border-radius-bottomleft: 15px;
        border-bottom-left-radius: 15px;
        width: 763px;
        height: 70px;
    }

    #MonthlyPresentationLeftPanel {
        clear: both;
        float: left;
        width: 70px;
        padding-left: 0px;
        margin: 5px 130px 0px 0px;
    }

    .PaginationLabel {
        clear: both;
        float: left;
        width: 30px;
        text-align: left;
        padding-top: 12px;
    }


    .LargeWidthSingleRow {
        width: 763px;
        height: 180px;
        overflow: auto;
        overflow-y: hidden;
        list-style: none;
        margin-top: 10px;
    }

    .LargeWidthDoubleRow {
        width: 763px;
        height: 330px;
        overflow: auto;
        overflow-y: hidden;
        list-style: none;
        margin-top: 10px;
    }


    .SmallWidthSingleRow {
        width: 733px;
        height: 170px;
        overflow: auto;
        overflow-y: hidden;
        list-style: none;
        margin-top: 10px;
    }

    .SmallWidthDoubleRow {
        width: 733px;
        height: 310px;
        overflow: auto;
        overflow-y: hidden;
        list-style: none;
        margin-top: 10px;
    }

    .SnapshotsBox {
        padding: 5px 5px 5px 15px;
        width: auto;
        list-style: none;
    }


    .DatalistPagination {
        clear: both;
        float: left;
        background-color: #DCE1E7;
        color: #808080;
        font-family: 'PT Sans', Arial;
        font-size: 12px;
        padding-left: 20px;
        font-weight: normal;
        padding-right: 10px;
        padding-bottom: 5px;
        border-top: 1px solid #96A2B7;
    }

        .DatalistPagination input {
            vertical-align: middle;
            padding-left: 1px;
        }

    .PresentationSelectedLabel {
        color: #666666;
        font-family: 'PT Sans', Arial;
        font-size: 11px;
        font-weight: normal;
        line-height: 13px;
        text-align: left;
        text-decoration: none;
    }

    .DatalistchannelPagination {
        clear: both;
        float: left;
        background-color: rgb(10, 100, 160);
        color: White;
        padding-left: 20px;
        font-weight: normal;
        padding-bottom: 5px;
        padding-right: 10px;
    }


</style>


<div id="gallery-black">
    <div class="imageContainerNone">

        @for (int i = 1; i < 11; i++)
        {

            <div class="image-block">

                @{ var val = i.ToString() + ".png"; }

                <a href="@Url.Action("ShowFilms", "Guest", new { id = i })">
                    <img src="~/Videos/Category/@val" height="155" width="155" />
                </a>



                <div class="reflection">
                    <img src='~/images/Category/@val' title="Category" height="155" width="155" alt="" />
                    <div class="overlay"></div>
                </div>



            </div>


        }
    </div>
</div>

