@model BarAndRestaurantMate.Models.GuestRoomAccountViewModel


@{
    ViewBag.Title = "Music";
    Layout = "~/Views/Shared/_LayoutMusic.cshtml";
}

<script src="~/Scripts/jquery-1.10.2.min.js"></script>

<script>
    $(function () {
        $("#searchSubmit").click(function ()
        {
            var searchEntry = $("#SearchName").val();


            $.ajax({
                url: "@Url.Action("GetMoviesList", "Guest")",
                data: { movie: searchEntry, catId : 8 },
            dataType: "html",
            type: "POST",
            error: function() {
                alert("An error occurred.");
            },
            success: function (data)
            {
                $("#MovieList").html("");
                $("#MovieList").html(data);
            }
        });
        });
    });
</script>


<div id="MovieList">

    @Html.Partial("_MusicVideosOnly")

 </div>

@Html.Partial("_SearchBox")

    @section SideBarBox
{

}




